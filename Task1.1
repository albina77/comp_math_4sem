import matplotlib.pyplot as plt

x0 = float(input())
n = int(input())


def func(r, n):
    if n != 0:
        x = func(r, n-1)
        return 4 * r * x * (1-x)
    else:
        return x0


r = 0.7
while [func(r, 2**7 + i) for i in range(2**n)] != [func(r, 2**7 + 2**n + k)  for k in range(2**n)] :
    print([func(r, 2**4 + i) for i in range(2**n)], [func(r, 2**4 + 2**n + k) for k in range(2**n)])
    print(r)
    r += 0.001
rn = r

print(rn)

